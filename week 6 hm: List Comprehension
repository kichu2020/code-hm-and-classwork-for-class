string = str(input("Enter your word:  "))
def vowels_remove(n):
  l2 = [i for i in string if i != "a" and i != "A" and i != "e" and i != "E" and i != "I" and i != "i" and i != "o" and i != "O" and i != "u" and i != "U" and i != " "]
  for j in l2:
    print(j,end = "")
  return l2
y = vowels_remove(string)

